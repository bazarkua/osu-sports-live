{
    "version": 2,
    "builds": [
      { "src": "client/package.json", "use": "@vercel/static-build" },
      { "src": "client/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "client/server.js" },
      { "src": "/(.*)", "dest": "client/$1"}
    ]
  }

  